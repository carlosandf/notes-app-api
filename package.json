  "dependencies": {
    "@sentry/node": "7.27.0",
    "@sentry/tracing": "7.27.0",
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
